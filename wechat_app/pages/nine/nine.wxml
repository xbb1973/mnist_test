<view class="container">   
  <view class="weui-grids">
    <view class="weui-grid" wx:for="{{routers}}" wx:key="name">  
      <navigator hover-class="other-navigator-hover" open-type="navigate" bindtap='toPage' id="{{item.id}}">  
        <view class="weui-grid__icon">  
          <image src=" {{item.icon}}" mode="scaleToFill"/>  
        </view>  
        <text class="weui-grid__label">{{item.name}}</text>  
      </navigator>  
    </view>  
  </view> 
<ad unit-id="adunit-652df6bf08179759" style="margin-top:5px;margin-bottom:5px"></ad>
<button class="up" open-type="contact" session-from="weapp">联系小帅 
</button>
</view>  