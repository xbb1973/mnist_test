page {
  height: 100%;
  color: #333;
  display: flex;
  flex-direction: column;
  font-family: PingFangSC-Regular;
  font: normal 30rpx/1.68 -apple-system-font,
          'Helvetica Neue',
          Helvetica,
          'Microsoft YaHei',
          sans-serif;
}
.user-probabilitys{
 text-align: center;
 font-family: 微软雅黑;
 color:#9b9b9b;
 font-weight: bold;
 font-size: 40rpx;
}
body, view {
  padding: 0;
  margin: 0;
  font-family: "PingFang SC",
                 "Helvetica Neue",
                 "hiragino sans gb",
                 Arial,
                 "Liberation Sans",
                 "Hiragino Sans GB",
                 "Source Han Sans CN",
                 "Source Han Sans SC",
                 "Microsoft YaHei",
                 "Wenquanyi Micro Hei",
                 "WenQuanYi Zen Hei",
                 "ST Heiti",
                 SimHei,
                 "WenQuanYi Zen Hei Sharp",
                 sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.image-box {
  /* height: 560rpx; */
  background-color: rgba(245, 247, 251, 1);
  background-position: center;
  background-size: contain;
  background-repeat: no-repeat;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.image {
  position: absolute;
  max-height: 100%;
  max-width: 100%;
}
.img_wrap {
    margin-bottom: 10px;
    width: 100%;
    height: 468rpx;
    background: #ececec;
}
image {
    width: 100%;
    height: 100%;
    max-height: 1
}
.image-min-box {
  position: relative;
  width: 100%;
}

.demo-result-title {
  display: flex;
  border-bottom: 1rpx solid #eee;
  height: 88rpx;
  color: #4a4a4a;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  align-items: center;
  padding-left: 40rpx;
}

.progress_box {
  position: relative;
  z-index: 1;
}

.progress_bg {
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
}

.progress_canvas {
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
}

.progress_info {
  height: 260rpx;
  font-size: 96rpx;
  line-height: 260rpx;
  color: #4a4a4a;
}

.progress_text {
  text-align: center;
}

.left {
  display: inline-block;
  padding: 20rpx 40rpx;
}

.right {
  display: inline-block;
  position: relative;
}

.little-title {
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  color: #4a4a4a;
  text-align: center;
}

.little-box {
  border-bottom: 1px solid rgba(238, 238, 238, 1);
  width: 100%;
}

.note-image {
  width: 60rpx;
  height: 60rpx;
}

.progress_part-box {
  float: left;
  position: relative;
}

.progress_part {
  width: 188rpx;
  height: 20rpx;
  float: left;
}

.part_title {
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  color: #9b9b9b;
  text-align: center;
  line-height: 54rpx;
}

.mild {
  background: #feefd8;
  border-bottom-left-radius: 10rpx;
  border-top-left-radius: 10rpx;
}

.moderate {
  background: #ffcfab;
}

.severe {
  background: #f5926a;
  border-bottom-right-radius: 10rpx;
  border-top-right-radius: 10rpx;
}

.pointer {
  position: absolute;
  bottom: 29rpx;
}

.pointer-image {
  width: 16rpx;
  height: 54rpx;
  animation: mymove 1s;
}

.lvhu-severe {
  display: flex;
  justify-content: center;
  padding: 30rpx;
}

.container-footer {
  position: fixed;
  width: 100%;
  margin: 10px auto;
  display: flex;
  height: 88rpx;
  justify-content: center;
  bottom: 20rpx;
}

button {
  border: none;
}

.one {
  display: block;
  text-align: center;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 34rpx;
  border-radius: 16rpx;
  color: #fff;
  background-color: rgb(30, 165, 233);
  width: 80%;
}

.hover-button {
  background: rgb(46, 140, 195);
}

.little-box:last-child {
  border-bottom: none;
}

.container {
  flex: 1;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}

.container-body {
  flex: 1;
  overflow-y: auto;
  overflow-x: hidden;
}

.tunRate {
  text-align: center;
  margin: 20rpx;
  font-size: 28rpx;
}

.dashang {
  color: #3eaeff;
  font-size: 32rpx;
}

.demo-result-content {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 32rpx;
}

.images-one {
  animation: mymove3 0.5s;
}

.feed-back {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  color: rgba(155, 155, 155, 1);
}

.demo-dibu {
  height: 164rpx;
}

@keyframes mymove {
  from {
    transform: translateX(-100rpx) scale(0.5) rotateX(90deg);
  }

  to {
    transform: translateX(0) scale(1) rotateX(0);
  }
}

@keyframes mymove3 {
  from {
    transform: translateY(800rpx) scale(0) rotateY(0deg);
  }

  to {
    transform: translateY(1) scale(1) rotateY(1);
  }
}
