 <view class="container">
<view class="img_wrap">
    <image src="{{ img }}" mode='aspectFit'/>
</view>
<button bindtap="uploads" class="up">拍照/选取图片识别</button>
  <text class="user-names" wx:if="{{classifyCnt}}">手势个数:{{classifyCnt}}</text>
  <text class="user-names" wx:if="{{label}}">手势名称:{{label}}</text>
  <text class="user-probabilitys" wx:if="{{confidence}}">对应的可信度:{{confidence}}</text>
  <text class="user-probabilitys" wx:if="{{info}}">错误提示:{{info}}</text>
 </view>